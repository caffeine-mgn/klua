TValue* klua_get_value(lua_State* L, int idx) {
  return index2value(L, idx);
}

void klua_push_value(lua_State* L, TValue* slot){
    setobj2s(L, L->top, slot);
    api_incr_top(L);
}

int klua_isvalid(lua_State* L,TValue* o){
    return (!ttisnil(o) || o != &G(L)->nilvalue);
}